{
    "name": "@iota-pico/examples-browser-ts",
    "version": "0.0.1",
    "description": "IOTA Pico Framework Examples for Browser using TypeScript",
    "engines": {
        "node": ">=8"
    },
    "scripts": {
        "build-clean": "rimraf ./dist/*",
        "build-compile": "tsc",
        "build-lint": "tslint -p ./tsconfig.json -c ./tslint.json -t verbose",
        "build-copy-html": "copyfiles --up 1 ./src/**/*.html ./dist",
        "build": "npm-run-all build-clean build-compile build-lint build-copy-html",
        "security": "nsp check",
        "dist": "npm-run-all build security"
    },
    "author": "martyn@iota.eco",
    "license": "MIT",
    "dependencies": {
        "@iota-pico/api": "^0.0.1",
        "@iota-pico/core": "^0.0.1",
        "@iota-pico/pal-browser": "^0.0.1",
        "systemjs": "^0.20.19"
    },
    "devDependencies": {
        "@types/node": "^9.3.0",
        "copyfiles": "^1.2.0",
        "npm-run-all": "^4.1.2",
        "nsp": "^3.1.0",
        "rimraf": "^2.6.2",
        "tslint": "^5.9.1",
        "tslint-microsoft-contrib": "^5.0.2",
        "typescript": "^2.6.2"
    }
}
